@{
    Layout = "~/Areas/student/Views/Shared/_main.cshtml";
}


<div class="content">
    <div class="module">
        <div class="module-head">
            <h3>社团信息</h3>
        </div>
        <div class="module-body">

            <br>
            <form class="form-horizontal row-fluid" action="">
                <div class="control-group">
                    <label class="control-label" for="basicinput">社团编号</label>
                    <div class="controls">
                        <input id="basicinput" placeholder="@Model.id" class="span8" disabled="" type="text">
                    </div>
                </div>
                <div class="control-group">
                    <label class="control-label" for="basicinput">社团名称</label>
                    <div class="controls">
                        <input id="basicinput" placeholder="@Model.name" class="span8" disabled="" type="text">
                    </div>
                </div>
                <div class="control-group">
                    <label class="control-label" for="basicinput">社团logo</label>
                    <div class="controls">
                        <img src="@Model.logo" class="span8" alt="社团logo" />
                    </div>
                </div>
                <div class="control-group">
                    <label class="control-label" for="basicinput">社团状态</label>
                    <div class="controls">
                        <input id="basicinput" placeholder="@Model.state" class="span8" disabled="" type="text">
                    </div>
                </div>
                <div class="control-group">
                    <label class="control-label" for="basicinput">社团创建日期</label>
                    <div class="controls">
                        <input id="basicinput" placeholder="@Model.date.ToString()" class="span8" disabled="" type="text">
                    </div>
                </div>
            </form>
        </div>
    </div>


</div><!--/.content-->
