<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />

    <title>Wopop</title>
    <link href="~/Content/login/style_log.css" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" type="text/css" href="~/Content/login/style.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/login/userpanel.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/login/jquery.ui.all.css" />

</head>

<body class="login" mycollectionplug="bind">
    <div class="login_m">
        <div class="login_logo"><img src="~/Content/login/logo.png" width="196" height="46" /></div>
        <div class="login_boder">
            <form action="~/student/Login/checkPwd">
                <div class="login_padding" id="login_model">

                    <h2>USERNAME</h2>
                    <label>
                        <input type="text" id="username" name="username" class="txt_input txt_input2" onfocus="if (value ==&#39;Your name&#39;){value =&#39;&#39;}" onblur="if (value ==&#39;&#39;){value=&#39;Your name&#39;}" value="Your name" />
                    </label>
                    <h2>PASSWORD</h2>
                    <label>
                        <input type="password" name="pwd" id="userpwd" class="txt_input" onfocus="if (value ==&#39;******&#39;){value =&#39;&#39;}" onblur="if (value ==&#39;&#39;){value=&#39;******&#39;}" value="******" />
                    </label>




                    <p class="forgot">
                        <label id="errorInfo" style="color:red;float:left;"></label>
                        <a id="iforget" href="javascript:void(0);">Forgot your password?</a>
                    </p>
                    <div class="rem_sub">
                        <div class="rem_sub_l">
                            <a href="/teacher/login/login">教师登录</a>&nbsp;&nbsp;
                            <a href="/manager/login/login">社团管理</a>
                        </div>
                        <label>
                            <input type="submit" class="sub_button" name="button" id="button" value="SIGN-IN" style="opacity: 0.7;" />
                        </label>
                    </div>
                </div>
            </form>

            <!--login_padding  Sign up end-->
        </div><!--login_boder end-->
    </div><!--login_m end-->
    <br /> <br />
    <script src="~/Content/js/common.js"></script>
    <Script language="javascript">
        window.onload=function(){
        errorInfo();
        }
    </Script>
</body>
</html>